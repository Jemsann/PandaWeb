@model PandaWeb.Models.EducationPlan

@{
    ViewBag.Title = "Edit";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Välkommen till @Html.DisplayFor(model => model.Name)</h2>
<h3>Meddelande om den här utbildningen</h3>
<hr />
<br />
<br />

<div name="info">
    <p>
        Den här utbildningen startar @Html.DisplayFor(model => model.StartDate) och avslutas @Html.DisplayFor(model => model.EndDate)
      <br />  Kurser i det här programmet:
        @Html.Action("CourseDetails")  </p>
</div>
<div name="UP">
    @Html.ActionLink("Utbildningsplan","UPSystemutveckling", "Education", new{ id=Model.EducationId }, null)
</div>


