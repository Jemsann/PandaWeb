@model IEnumerable<PandaWeb.Models.EducationPlan>

@{
    ViewBag.Title = "Backendutveckling";
    Layout = "~/Views/Shared/_LayoutEducation.cshtml";
}
<p>
    <br />
    <br />

</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
